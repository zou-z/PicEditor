<Application x:Class="PicEditor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PicEditor"
             xmlns:converter="clr-namespace:PicEditor.Converter"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <converter:EditModeConverter x:Key="editModeConverter"/>
            <converter:LeftToMarginConverter x:Key="leftToMarginConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
